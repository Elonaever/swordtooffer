//动态规划
class Solution {
public:
 int Fibonacci(int n) {
 if(n==0) return 0;
 int ans=1;
 int pa=0;
while(--n){
    ans += pa;
    pa = ans -pa;
}
 return ans;
}
};


//递归调用时间太长

//临时开辟数组
class Solution {
public:
    int Fibonacci(int n) {
	static int fib[40]={0,1};
	if(fib[39]!= 0) return fib[n];
	for(int i = 2;i<=n;++i){
		fib[i] = fib[i-1] + fib[i-2];
	}
	return fib[n];
    }
};
